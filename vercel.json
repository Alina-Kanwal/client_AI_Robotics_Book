{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets|static|favicon.ico|sitemap.xml|.*\\.).*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm install && npx docusaurus build",
  "outputDirectory": "build"
}